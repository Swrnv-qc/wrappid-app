{
  "name": "wrappid-app",
  "version": "0.0.1",
  "description": "This is a boilerplate application using wrappid for both web and mobile application.",
  "scripts": {
    "code:lint": "eslint --ignore-path .gitignore",
    "code:format": "npm run code:lint -- --fix",
    "prepare": "husky install",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "setup:mobile": ".wrappid\\script\\windows\\setup_mobile.bat",
    "setup:web": ".wrappid\\script\\windows\\setup_web.bat",
    "start:mobile": "npm start --prefix .wrappid\\temp\\mobile",
    "start:web": "npm start --prefix .wrappid\\temp\\web",
    "src:copy:mobile": ".wrappid\\script\\windows\\copy_app_src_mobile.bat",
    "src:copy:web": ".wrappid\\script\\windows\\copy_app_src_web.bat",
    "reload:styles:mobile": ".wrappid\\script\\windows\\copy_styles_dist_mobile.bat",
    "reload:styles:web": ".wrappid\\script\\windows\\copy_styles_dist_mobile.bat",
    "reload:sc:mobile": ".wrappid\\script\\windows\\copy_styled_comp_dist_mobile.bat",
    "reload:sc:web": ".wrappid\\script\\windows\\copy_styled_comp_dist_mobile.bat",
    "reload:core:mobile": ".wrappid\\script\\windows\\copy_core_dist_mobile.bat",
    "reload:core:web": ".wrappid\\script\\windows\\copy_core_dist_mobile.bat"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wrappid/wrappid-app.git"
  },
  "keywords": [
    "wrappid",
    "react",
    "react-native",
    "hybrid",
    "javascript"
  ],
  "author": "wrappid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wrappid/wrappid-app/issues"
  },
  "homepage": "https://github.com/wrappid/wrappid-app#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt-cli": "^17.5.0",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@wrappid/core": "^0.0.11",
    "eslint": "^8.37.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "standard-version": "^9.5.0"
  }
}